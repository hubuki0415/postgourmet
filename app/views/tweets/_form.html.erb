<%= form_with(model: tweet, local: true) do |form| %>
      <%= form.text_area :text, placeholder: "text", rows: "10" %>
        <div class="click-upload">
        <p>
          クリックしてファイルをアップロード
        </p>
        <%= form.file_field :image, id:"item-image" %>
      </div>
      <%= form.submit "SEND" %>
    <% end %>